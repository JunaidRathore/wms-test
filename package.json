{
  "name": "nashos-wms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:https": "next dev --hostname 0.0.0.0 --port 3000 --experimental-https --experimental-https-key ./certificates/localhost-key.pem --experimental-https-cert ./certificates/localhost.pem",
    "dev:with-https": "concurrently \"npm run dev\" \"npm run dev:https\"",
    "dev:proxy": "concurrently \"next dev --port 3000 --hostname 0.0.0.0\" \"local-ssl-proxy --source 3001 --target 3000\"",
    "dev:proxy-alt": "concurrently \"next dev --port 8080 --hostname 0.0.0.0\" \"local-ssl-proxy --source 8443 --target 8080\"",
    "dev:tunnel": "concurrently \"next dev --port 3000\" \"lt --port 3000 --print-requests --local-host localhost --custom-user-agent 'Mozilla/5.0 (NashOS-WMS-Dev)' --subdomain nashos-wms-dylan-dev\"",
    "generate-cert": "node generate-cert.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.6.0",
    "@prisma/client": "^6.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@undecaf/zbar-wasm": "^0.11.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.464.0",
    "next": "14.0.3",
    "next-themes": "^0.4.3",
    "react": "^18",
    "react-dom": "^18",
    "react-qr-barcode-scanner": "^2.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "20.17.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8",
    "prisma": "^6.0.1",
    "selfsigned": "^2.4.1",
    "tailwindcss": "^3.3.0",
    "typescript": "5.7.2"
  }
}
